{{- if eq .Type ".css" -}}
	{{- if .Resource.Embed -}}
		<style{{if .Resource.Attr}}{{range .Resource.Attr}} {{.Name}}="{{.Value}}"{{end}}{{end}}>{{.Content}}</style>
	{{- else -}}
		<link href="{{link "resource" .Resource.Name}}" rel="stylesheet" type="text/css"{{if .Resource.Attr}}{{range .Resource.Attr}} {{.Name}}="{{.Value}}"{{end}}{{end}} />
	{{- end -}}
{{- else if eq .Type ".js" -}}
	{{- if .Resource.Embed -}}
		<script{{if .Resource.Attr}}{{range .Resource.Attr}} {{.Name}}="{{.Value}}"{{end}}{{end}}>{{.Content}}</script>
	{{- else -}}
		<script src="{{link "resource" .Resource.Name}}" charset="utf-8"{{if .Resource.Attr}}{{range .Resource.Attr}} {{.Name}}="{{.Value}}"{{end}}{{end}}></script>
	{{- end -}}
{{- else -}}
	{{- if .Resource.Embed -}}
		{{.Content}}
	{{- end -}}
{{- end -}}
